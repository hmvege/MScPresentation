	pages = {93--150},
	annote = {Comment: A typo in Eq. (3.9) has been corrected. An additional subsection (5.2) has been inserted to demonstrate the nonrenormalizability of the relevant theta parameter in the presence of massive fermions, which implies that the continuum (a -{\textgreater} 0) limit must be taken keeping theta fixed},
	file = {arXiv\:0803.1593 PDF:/Users/hansmathiasmamenvege/Zotero/storage/M5HZU2XV/Vicari and Panagopoulos - 2009 - Theta dependence of SU(N) gauge theories in the pr.pdf:application/pdf;arXiv.org Snapshot:/Users/hansmathiasmamenvege/Zotero/storage/JNSH4FRY/0803.html:text/html}
}

